{
	"name": "openfusionapi",
	"version": "0.0.4",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"start": "node ./server/index.js",
		"build_start": "vite build && node ./server/index.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@edwinspire/libopenfusionapi": "https://github.com/edwinspire/libOpenFusionAPI.git",
		"@edwinspire/libopenfusionapigui": "https://github.com/edwinspire/libOpenFusionAPIGUI.git",
		"@sveltejs/adapter-static": "^3.0.1",
		"jsonwebtoken": "^9.0.0"
	}
}
