{
	"name": "openfusionapi",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"start": "node ./server/index.js",
		"build_start": "vite build && node ./server/index.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@codemirror/lang-javascript": "^6.1.9",
		"@codemirror/lang-json": "^6.0.1",
		"@codemirror/lang-sql": "^6.5.0",
		"@codemirror/lint": "^6.2.2",
		"@edwinspire/libopenfusionapi": "github:edwinspire/libOpenFusionAPI",
		"@edwinspire/svelte-components": "github:edwinspire/svelte-components",
		"@sveltejs/adapter-static": "^3.0.1",
		"bulma": "^0.9.4",
		"codemirror": "^6.0.1",
		"d3": "^7.8.5",
		"jsonwebtoken": "^9.0.0"
	}
}
